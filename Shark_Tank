//c++ program for day 12
#include <iostream>
using namespace std;

int main() {
    int t, i, n, m, x, y;
    cout << "enter testcase: ";
    cin >> t;
    
    if (t<=100 && t>=1) 
    {
        for(i=0;i<t;i++)
        {
            cin >> n >> m;
            if(n>=100 && m>=100 && m<=10000 && n<=10000)
            {
                x = n*10;
                y= m*5;
                if(x>y)
                cout << "first";
                else if (y>x)
                cout << "second";
                else cout <<"any";
                
            }
            else cout <<"invalid input\n";
        }
    }
    else cout << "input invalid\n";
    return 0;
}
