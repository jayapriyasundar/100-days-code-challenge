//c++ program for day 1
#include <iostream>
using namespace std;

int main() {
    int a, i, x, y, z;
    cout << "enter testcase: ";
    cin >> a;
    
    if (a<=1000 && a>=1) 
    {
        for(i=0;i<a;i++)
        {
            cin >> x >> y >> z;
            if (x<=1000 && x>=1 && y<=1000 && y>=1 && z<=1000 && z>=1)
            cout << x*y*z << endl;
            else cout << "invalid input\n";
        }
    }
    else cout << "invalid input\n";
    return 0;
    }
