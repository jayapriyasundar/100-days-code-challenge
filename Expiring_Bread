//c++ program for day 6
#include <iostream>
using namespace std;

int main() {
    int t, i, n, k,m;
    
    
    cout << "enter testcase: ";
    cin >> t;
    
    if (t<=1000 && t>=1) 
    {
        for(i=0;i<t;i++)
        {
            cin >> n >> m >> k;
            if(n>=1 && m>=1 && k>=1 && k<=100 && m<=100 && n<=100)
            {
                if( n <= m*k) 
                cout <<"yes\n";
                else cout <<"no\n";
            }
            else cout <<"invalid input\n";
        }
    }
    else cout << "input invalid\n";
    return 0;
}
