
// c program to print all sum combinations of three numbers by taking two numbers at a time.

#include <stdio.h>



int main() {

   int a,b,c;

   //getting 3 input from user

   printf("enter first number:\n");

   scanf("%d",&a);

   printf("enter second number:\n");

   scanf("%d",&b);

   printf("enter third number:\n");

   scanf("%d",&c);

   

   //printing the output

   

   printf("the sum are:\n%d\n%d\n%d\n",a+b,b+c,a+c);

   



    return 0;

}
